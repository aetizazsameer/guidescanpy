{
  "guidescan": {
    "bin": "guidescan",
    "grna_database_path_prefix": "{BAMS_PATH}",
    "grna_database_path_map": {
      "ce11": {
        "cas9": "cas9/ce11.bam.sorted",
        "cpf1": "cpf1/ce11.bam.sorted"
      },
      "mm10": {
        "cas9": "cas9/mm10.bam.sorted"
      }
    },
    "db": "host={POSTGRES_HOST} dbname={POSTGRES_DB} user={POSTGRES_USER} password={POSTGRES_PASSWORD}"
  },

  "flask": {
    "DEBUG": 0,
    "SECRET_KEY": "Dfsf00_34aslaS~~t5))pVls0$aGkxlas[q%91;",
    "SQLALCHEMY_TRACK_MODIFICATIONS": 0
  },

  "celery": {
    "broker": "{CELERY_BROKER}",
    "backend": "{CELERY_BACKEND}"
  },

  "logging": {
    "version": 1,
    "formatters": {
      "simple_formatter": {
        "format": "%(asctime)s %(levelname)-8s - %(message)s",
        "datefmt": "%Y/%m/%d %H:%M:%S"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "simple_formatter",
        "level": "DEBUG",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "guidescan": {
        "level": "DEBUG",
        "handlers": ["console"]
      }
    }
  }
}